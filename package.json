    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.22",
    "@types/react-test-renderer": "18.0.7",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "bundlesize": "0.18.2",
    "css-loader": "6.10.0",
    "esbuild": "0.20.2",
    "esbuild-css-modules-plugin": "^2.7.1",
    "eslint": "8.57.0",
