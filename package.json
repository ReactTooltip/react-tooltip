    "prettier": "3.2.5",
    "process": "^0.11.10",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "rimraf": "5.0.5",
    "rollup": "4.13.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-browsersync": "^1.3.3",
    "rollup-plugin-copy": "3.5.0",
    "rollup-plugin-dts": "6.1.0",
